{
    "manifest_version": 3,
    "name": "WhatsApp Sidebar Toggle",
    "version": "1.0",
    "description": "Toggle the sidebar in WhatsApp Web",
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://web.whatsapp.com/*"],
        "js": ["content.js"]
      }
    ]
  }